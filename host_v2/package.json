            "**/.storybook/**/*.*",
            "@righton/networking/*"
          ],
          "peerDependencies": true
        }
      ],
      "react/jsx-props-no-spreading": [
        "off"
      ],
      "react/require-default-props": [
        "off"
      ],
      "arrow-body-style": [
        "off"
      ],
      "no-empty-pattern": [
        "off"
      ],
      "no-console": [
        1,
        {
          "allow": [
            "debug",
            "error"
          ]
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "*.test.tsx"
        ],
        "rules": {
          "@typescript-eslint/no-non-null-assertion": "off"
        }
      }
    ]
